{
  "private": true,
  "name": "remix-cloudflare-prisma",
  "description": "",
  "license": "",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "remix-esbuild-override",
    "setup": "npm ci && npm run prisma && npm run build",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:prisma": "PRISMA_CLIENT_ENGINE_TYPE=dataproxy prisma generate",
    "build:tailwind": "tailwindcss -i ./app/tailwindInput.css -o ./app/tailwind.css",
    "build:remix": "remix build",
    "dev": "pm2-dev ./pm2.config.js",
    "update": "run-p update:** && npm i",
    "update:prisma:run": "npm i @prisma/client @prisma/generator-helper",
    "update:prisma:dev": "npm i --save-dev prisma prettier-plugin-prisma",
    "update:typescript-eslint": "npm i --save-dev @typescript-eslint/eslint-plugin @typescript-eslint/parser"
  },
  "dependencies": {
    "@firebase/app": "^0.7.25",
    "@firebase/auth": "^0.20.2",
    "@headlessui/react": "^1.6.4",
    "@keystone-6/document-renderer": "^1.0.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@paypal/paypal-js": "^5.0.6",
    "@prisma/client": "^3.14.0",
    "@prisma/generator-helper": "^3.14.0",
    "@remix-run/cloudflare": "^1.5.1",
    "@remix-run/cloudflare-pages": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@sendgrid/mail": "^7.7.0",
    "@ssttevee/cfw-formdata-polyfill": "^0.2.1",
    "@tailwindcss/forms": "^0.5.2",
    "@theme-toggles/react": "^4.1.0",
    "array.prototype.groupby": "^1.1.0",
    "classnames": "^2.3.1",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "esbuild": "0.14.42",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-register": "^3.3.2",
    "firebase": "^9.8.2",
    "firebase-admin": "https://github.com/acoreyj/firebase-admin-cloudflare/raw/master/firebase-admin-10.2.0-4.tgz",
    "firebaseui": "^6.0.1",
    "nanoid": "^3.3.4",
    "pm2": "^5.2.0",
    "postcss": "^8.4.14",
    "prisma": "^3.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^5.0.3",
    "react-firebaseui": "^6.0.0",
    "react-hot-toast": "^2.2.0",
    "react-icons": "^4.4.0",
    "react-loading-icons": "^1.1.0",
    "tailwindcss": "^3.0.24",
    "wrangler": "beta"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.11.0",
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/grecaptcha": "^3.0.4",
    "@types/paypal__checkout-server-sdk": "^1.0.4",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.7",
    "daisyui": "^2.15.2",
    "esbuild": "0.13.14",
    "esbuild-plugin-replace": "^1.2.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-template-curly-in-string-fix": "^1.0.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-prisma": "^3.14.0",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "prisma": "^3.14.0",
    "remix-esbuild-override": "^3.0.4",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.7.2"
  },
  "overrides": {},
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "module": "./src/index.mjs",
  "husky": {
    "hooks": {
      "pre-commit": "CI=1 lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|mjs|ts|tsx|css|md|yml)": [
      "prettier --write"
    ]
  }
}
